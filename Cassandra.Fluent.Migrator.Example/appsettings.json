{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Level:u3}] - [{Timestamp:HH:mm:ss}] - [<s:{SourceContext}>] - [{Message:lj}] {NewLine}{Exception}"
        }
      }
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  },

  "Cassandra": {
    "ContactPoints": [],
    "Port": 0,
    "Credentials": {
      "UserName": "",
      "Password": ""
    },
    "DefaultKeyspace": "",
    "Replication": {
      "class": "",
      "replication_factor": 3,
      "datacenter": ""
    },
    "Query": {
      "ConsistencyLevel": "",
      "HeartBeat": 0
    }
  }
}
